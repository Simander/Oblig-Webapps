@model List <NettbutikkMVC.Models.Kunde>

@{
    ViewBag.Title = "RegistrertKundeOK";
}
<!DOCTYPE html>
<html>
    <head>
        <title>ListAlleKunder</title>
    </head>
    <body>
        <div>
            <h3>
                Her er brukerne i databasen:
            </h3>
            <table>

                @foreach (var kunde in Model)
                {
                    <tr>
                        <td>@kunde.KundeNR</td>
                        <td>@kunde.Fornavn</td>
                        <td>@kunde.Etternavn</td>
                        <td>@kunde.Adresse</td>
                       
                   
                    </tr>
                }
            </table>
        </div>
    </body>

</html>


